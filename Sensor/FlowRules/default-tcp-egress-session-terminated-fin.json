{
    "id": "default-tcp-egress-session-terminated-fin",
    "description": "Detects a graceful TCP session termination initiated by an internal host.",
    "severity": "info",
    "sequence": [
        {
            "index": "FinAck-Request",
            "condition": { "tcp": { "flags": ["F", "A"] }},
            "direction": "out",
            "goto": "FinAck-Response"
        },
        {
            "index": "FinAck-Response",
            "condition": { "tcp": { "flags": ["F", "A"] }},
            "direction": "in",
            "timestamp": { "nano": 5000000000 },
            "action": {
                "type": "notice",
                "message": "Egress TCP session gracefully terminated (FIN)"
            }
        }
    ]
}