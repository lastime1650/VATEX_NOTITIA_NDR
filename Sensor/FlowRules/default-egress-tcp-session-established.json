{
    "id": "default-egress-tcp-session-established",
    "description": "Detects a TCP session initiated from the internal network (outbound connection).",
    "severity": "info",
    "sequence": [
        {
            "index": "Syn",
            "condition": { "tcp": { "flags": ["S"] }},
